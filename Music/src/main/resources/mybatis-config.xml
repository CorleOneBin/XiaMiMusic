<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
        PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>

    <typeAliases>
        <typeAlias type="cn.xiami.module.Category" alias="_Category"/>
        <typeAlias type="cn.xiami.module.Cinfo" alias="_Cinfo"/>
        <typeAlias type="cn.xiami.module.Music" alias="_Music"/>
        <typeAlias type="cn.xiami.module.User" alias="_User"/>
        <typeAlias type="cn.xiami.module.Admin" alias="_Admin"/>
